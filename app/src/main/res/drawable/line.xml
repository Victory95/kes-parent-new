<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="35"
            android:pivotX="45%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="1dp"
                    android:color="@color/black"/>
            </shape>
        </rotate>
    </item>
</layer-list>